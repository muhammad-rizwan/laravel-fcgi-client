{
  "name": "rizwan/laravel-fcgi-client",
  "description": "A Laravel package for communicating with LaravelFcgiClient servers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Rizwan",
      "email": "rizwan.ciit@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "orchestra/testbench": "^8.0",
    "laravel/pint": "^1.22"
  },
  "autoload": {
    "psr-4": {
      "Rizwan\\LaravelFcgiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rizwan\\LaravelFcgiClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rizwan\\LaravelFcgiClient\\FcgiClientServiceProvider"
      ],
      "aliases": {
        "FCGI": "Rizwan\\LaravelFcgiClient\\Facades\\FCGI"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
